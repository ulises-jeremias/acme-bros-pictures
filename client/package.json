{
  "name": "acme-bros-pictures",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack",
    "build:dev": "yarn build --mode development",
    "build:dev:watch": "yarn build:dev --watch",
    "build:dev:bundleanalyze": "yarn build:dev --addons=bundleanalyze",
    "build:dev:bundlebuddy": "yarn build:dev --addons=bundlebuddy",
    "build:prod": "yarn build -p",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:bundleanalyze": "yarn build:prod --addons=bundleanalyze",
    "build:prod:bundlebuddy": "yarn build:prod --addons=bundlebuddy",
    "upgrade:package": "yarn upgrade-interactive --latest",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "lint:fix": "eslint src/**/*.js src/**/*.jsx --fix",
    "serve:dev": "webpack-dev-server --mode development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --addons=dashboard",
    "serve:prod": "yarn build:prod && live-server ./dist",
    "start": "yarn serve:dev",
    "test": "jest --config .jest.config.js",
    "test:ci": "yarn test --ci",
    "test:watch": "jest --config .jest.config.js --watch",
    "webpack-defaults": "webpack-defaults",
    "docker:dev": "yarn docker:build && yarn docker:start",
    "docker:build": "docker build -f docker/Dockerfile --target development -t acme-bros-pictures .",
    "docker:start": "docker run --rm -it --network host -v $PWD:/usr/src/app acme-bros-pictures",
    "docker:prod": "yarn docker:build:prod && yarn docker:start:prod",
    "docker:build:prod": "docker build --build-arg API_BASE_URL=$API_BASE_URL -f docker/Dockerfile --target production -t acme-bros-pictures:production .",
    "docker:start:prod": "docker run --rm -it --network host -e API_BASE_URL=$API_BASE_URL -v $PWD:/usr/src/app acme-bros-pictures:production"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "connected-react-router": "^6.5.2",
    "copy-webpack-plugin": "^5.0.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^19.0.0",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "inject-html-webpack-plugin": "^0.0.39",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.16",
    "react-i18next": "7.9.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-semantic-toasts": "^0.6.5",
    "redux": "^4.0.4",
    "redux-cookie": "^0.5.9",
    "redux-devtools": "^3.5.0",
    "redux-devtools-chart-monitor": "^1.7.0",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-filterable-log-monitor": "^0.8.1",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-devtools-multiple-monitors": "^1.0.1",
    "redux-form": "^8.2.6",
    "redux-form-validators": "^3.3.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "redux-slider-monitor": "^2.0.0-2",
    "redux-thunk": "^2.3.0",
    "semantic-ui-calendar-react": "^0.15.3",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "terser-webpack-plugin": "^2.2.1",
    "underscore": "^1.9.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dashboard": "^3.2.0",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@types/moment-timezone": "^0.5.12",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "less": "2.7.3",
    "less-loader": "^5.0.0",
    "live-server": "^1.2.1",
    "node-sass": "^7.0.0",
    "reducer-maker": "^0.0.8",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "style-loader": "^1.0.0",
    "url-loader": "1.1.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
